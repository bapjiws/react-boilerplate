{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/bapjiws/react-boilerplate.git",
  "author": "bap jiws",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "node-sass": "^4.6.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.7.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "next",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.2.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "express": "^4.15.3"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "prettier-eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "lint-js": "eslint . --ext .js",
    "lint-js:fix": "eslint . --ext .js --fix",
    "lint-css": "stylelint '**/*.scss'",
    "lint-css:fix": "stylelint '**/*.scss' --fix",
    "precommit": "lint-staged",
    "postcommit": "git reset *",
    "start:dev": "node server.js",
    "start:dev-live": "nodemon server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "release-the-kraken": "yarn run build && yarn run prod"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
